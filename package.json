{
  "name": "tumblepub",
  "version": "0.1.0",
  "repository": "https://github.com/sand-head/tumblepub.git",
  "author": "Justin Schweigert <justin@schweigert.dev>",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "concurrently \"yarn:build:web:watch\" \"yarn:start:api\"",
    "build": "yarn build:web && yarn build:api",
    "start:web": "snowpack dev",
    "build:web:watch": "snowpack build --watch",
    "build:web": "snowpack build",
    "start:api": "cargo run",
    "build:api": "cargo build"
  },
  "dependencies": {
    "@bikeshaving/crank": "^0.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@snowpack/plugin-babel": "^2.1.3",
    "@snowpack/plugin-dotenv": "^2.0.3",
    "@snowpack/plugin-typescript": "^1.0.1",
    "@types/snowpack-env": "^2.3.0",
    "concurrently": "^5.3.0",
    "snowpack": "^2.13.3",
    "typescript": "^4.0.3"
  }
}
